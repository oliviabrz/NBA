https://www.programmersought.com/article/7272665245/
sudo wget ftp://ftp.unixodbc.org/pub/unixODBC/unixODBC-2.3.9.tar.gz
 
tar -xvzf unixODBC-2.3.9.tar.gz 
 
cd unixODBC-2.3.9/
 
sudo ./configure --sysconfdir=/etc
sudo make
sudo make install

// Registers the Unicode driver:
sudo myodbc-installer -a -d -n "MySQL ODBC 8.0 Driver" -t "Driver=/usr/local/lib/libmyodbc8w.so"
 
// Registers the ANSI driver 
sudo myodbc-installer -a -d -n "MySQL ODBC 8.0 ANSI Driver" -t "Driver=/usr/local/lib/libmyodbc8a.so"

